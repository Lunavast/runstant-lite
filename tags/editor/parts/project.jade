project
  div.preview

  style.
    project {
    
    }
    project .preview {
      width: 100%;
      height: 100%;
      overflow: scroll;
      -webkit-overflow-scrolling: touch;
    }
    project .preview iframe {
      width: 100%;
      height: 100%;
      border: none;
    }

  script.
    var self = this;

    this.on('mount', function() {
      this.refresh();
    });

    this.refresh = function() {
      if (!self.jframe) {
        self.jframe = jframe('project .preview');
      }
      var v = runstant.project.toProject();
      self.jframe.load(v);
    };



